     <div className=\"data-view\">\n                <h2>{props.playData.title}</h2>\n                <article id=\"actHere\">\n                    <h3>{props.currentAct.name}</h3>\n                    <div id=\"sceneHere\">\n                        <h4>{props.currentScene.name}</h4>\n                        <p>{props.currentScene.title}</p>\n                        <p>{props.currentScene.stageDirection}</p>\n                        {props.currentScene.speeches.filter((s) => \n                            //if 'all players' is selected return all speeches\n                            //else return speeches based on current character\n                            props.currentCharacter === \"All Players\" ? true : props.currentCharacter === s.speaker\n                        ).map((s, i) => \n                            <div key={i}>\n                                <p className=\"speaker\"><strong>{s.speaker}</strong></p>\n                                {s.lines.map((l, i) => \n                                    <p className=\"line\" key={i}>\n                                         <Highlighter searchWords={[props.searchText]} textToHighlight={l}/>\n                                    </p>\n                                )}\n                            </div>\n                        )}\n                    </div>\n                </article>\n            </div>\n        );\n    }\n};\n\nexport default PlayDataView",{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":2,"column":9,"nodeType":"83","messageId":"84","endLine":2,"endColumn":27},{"ruleId":"81","severity":1,"message":"85","line":6,"column":11,"nodeType":"83","messageId":"84","endLine":6,"endColumn":24},"no-native-reassign",["86"],"no-negated-in-lhs",["87"],"no-unused-vars","'CSSTransitionGroup' is defined but never used.","Identifier","unusedVar","'highlightText' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]